{
  "name": "turbo-telegram",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "babel-node ./bin/www",
    "client": "cd client && yarn start",
    "watch": "nodemon ./bin/www",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "dev": "concurrently --kill-others-on-fail \"yarn watch\" \"yarn client\" "
  },
  "dependencies": {
    "@blueprintjs/docs-theme": "^3.0.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "mongoose": "^5.2.5",
    "morgan": "~1.9.0",
    "nodemon": "^1.18.3"
  },
  "devDependencies": {
    "babel-watch": "^2.0.7"
  }
}
